{{>layout/header}}

<h1>회원 목록</h1>
<table class="table table-hover table-horizontal table-bordered">
    <thead class="thead-strong">
    <tr>
        <th>학년</th>
        <th>반</th>
        <th>이름</th>
        <th>전화번호</th>
    </tr>
    </thead>
    <tbody id="tbody">
    {{#member}}
        <tr data-href="/members/member/detail/{{id}}">
            <td>{{grade.title}}</td>
            <td>{{className.title}}</td>
            <td>{{name}}</td>
            <td>{{phone}}</td>
        </tr>
    {{/member}}
    </tbody>
</table>

{{>layout/footer}}
<script src="/js/app/row-click.js"></script>